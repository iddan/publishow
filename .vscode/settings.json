{
  "prettier.bracketSpacing": true,
  "prettier.eslintIntegration": true,
  "prettier.tabWidth": 2,
  "[javascript]": {
    "editor.formatOnSave": true,
  },
  "[javascriptreact]": {
    "editor.formatOnSave": true,
  },
  "eslint.enable": true,
  "files.exclude": {
    "node_modules": "explorerExcludedFiles",
    "coverage": "explorerExcludedFiles",
    "build": "explorerExcludedFiles",
    ".DS_Store": "explorerExcludedFiles",
    ".env.local": "explorerExcludedFiles",
    ".env.development.local": "explorerExcludedFiles",
    ".env.test.local": "explorerExcludedFiles",
    ".env.production.local": "explorerExcludedFiles",
    "npm-debug.log*": "explorerExcludedFiles",
    "yarn-debug.log*": "explorerExcludedFiles",
    "yarn-error.log*": "explorerExcludedFiles"
  }
}
